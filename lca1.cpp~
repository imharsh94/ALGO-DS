#include<bits/stdc++.h>
using namespace std;

struct Node
{
    int key;
    struct Node *left,*right;
};

Node * newNode(int k)
{
    Node *temp = new Node;
    temp->key = k;
    temp->left = temp->right = NULL;
    return temp;
}

