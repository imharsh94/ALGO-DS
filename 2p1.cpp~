#include<bits/stdc++.h>
using namespace std;

int l,r;

bool f(int a[],sum)
{
    while( l<r)
    {
	if(a[l] + a[r] == sum)
    }
}

int main()
{
    int n,sum;
    cin>>n>>sum;
    int a[n];
	
    cin>>a[n];
    for(int i=0; i<n; i++)
	cin>>a[i];

    sort(a,a+n);

    l=0;r=n-1;

   int ans = f(a);
    return 0;
}
